
{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "/index.js" 
      }
    ],
    "functions": {
      "composer": {
        "runtime": "@vercel/node",
        "memory": 128,
        "maxDuration": 5000,
        "includeFiles": [
          "composer.phar"
        ],
        "handler": "composer.phar"
      }
    }
  }